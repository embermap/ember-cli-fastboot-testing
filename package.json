    "node": "12.* || 14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-fastboot"
    ],
    "after": []
  },
  "fastbootDependencies": [
    "crypto"
  ]
}
