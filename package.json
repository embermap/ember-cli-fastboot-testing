{
  "name": "ember-cli-fastboot-testing",
  "version": "0.5.0",
  "description": "Test your FastBoot-renderedÂ HTML alongside your application's tests.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/embermap/ember-cli-fastboot-testing",
  "license": "MIT",
  "author": "Ryan Toronto <ryanto@gmail.com> (https://embermap.com)",
  "homepage": "https://embermap.github.io/ember-cli-fastboot-testing",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:separate-build": "yarn build --environment test && yarn test --path=dist --filter 'Fastboot | basic'",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "ember-auto-import": "^1.2.15",
    "ember-cli-babel": "^7.26.6",
    "fastboot": "^3.0.3",
    "json-fn": "^1.1.1",
    "minimist": "^1.2.0",
    "nock": "^12.0.3",
    "resolve": "^1.10.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.5.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-plugin": "^4.0.1",
    "broccoli-stew": "^3.0.0",
    "ember-cli": "~3.28.3",
